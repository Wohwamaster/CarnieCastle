pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--global directions
north=0
east=1
west=2
south=3

--temp variable used to simulate turn based movement
pturn=true

--table for player containing direction and sprite
player={
	direct = north,
	sp=1
}

function _init()
	--sets up gameboard with nil values
	gb={}
	for i=1,16 do
		gb[i]={}
		for j=1,16 do
			gb[i][j]=nil
		end
	end
	--put player at position [2][2] in gb
	gb[2][2]=player
end

function _update()
	if pturn then
		--move player
		for i=1,16 do --iterate through gb to find player
			for j=1,16 do
				if gb[i][j]==player then

					if btn(0) then
						if player.direct==west then
							if i-1 == 1 then --stops movement
								gb[i][j]=player
							else
								--move player 1 space left
								gb[i-1][j]=player
								gb[i][j]=nil
							end
						else
							--set direction to west
							player.direct=west
						end
						pturn=false
					end

					if btn(1) then
						if player.direct==east then
							if i+1 == 16 then --stops movement
								gb[i][j]=player
							else
								--move player 1 space right
								--is currently moving player to gb[15][j]
								gb[i+1][j]=player
								gb[i][j]=nil
							end
						else
							--set direction to east
							player.direct=east
						end
						pturn=false
					end

					if btn(2) then
						if player.direct==north then
							if j-1 == 1 then --stops movement
								gb[i][j]=player
							else
								--move player 1 up
								gb[i][j-1]=player
								gb[i][j]=nil
							end
						else
							--set direction to north
							player.direct=north
						end
						pturn=false
					end

					if btn(3) then
						if player.direct == south then
							if j+1 == 17 then --stops movement
								gb[i][j]=player
						 else
								--moves player 1 down
								gb[i][j+1]=player
								gb[i][j]=nil
							end
						else
							--set direction to south
							player.direct=south
						end
						pturn=false
					end
					--breaks loop if the player is found
					break
				end
			end
		end
	end

	--resets pturn
	if btn(4) then
		pturn=true
	end
end

function _draw()
	cls()
	rectfill(0,0,128,128,3)
	rectfill(0,0,0,0,7)
	--everything below this point is for debugging purposes
	print(gb[2][2])
	print(gb[2][3])
	print(counter)
	print(pturn)
	print(player.direct)

	for i=1,16 do
		for j=1,16 do
			if gb[i][j]==player then
				spr(player.sp, i*8, j*8)
				print(i)
				print(j)
			end
		end
	end

end
